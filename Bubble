#2
def bubbleSort(lista ,tam):
	troca=True
	while troca:
		troca= False
		for i in range(tam-1):
			if lista[i] > lista[i+1]:
				chave = lista[i]
				lista[i] = lista[i+1]
				lista[i+1] = chave
				troca= True
	return lista
lista=[]
for i in range(0, 10):
	n=int(input('Digite um nÃºmero: '))
	lista.append(n)
tam=len(lista)
print(lista)

print(bubbleSort(lista ,tam))